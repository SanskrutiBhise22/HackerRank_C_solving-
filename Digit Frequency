#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    char num[1000],s='0';
    scanf("%s",num);
    for(int j=0;j<10;j++){
        int count=0;
        for(int i=0;num[i]!='\0';i++){
           if(num[i]==s){
              count++;
           }
        }
        s++;
        printf("%d ",count);
    }
    return 0;
}
